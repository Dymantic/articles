{
    "name": "dymantic/articles",
    "description": "Laravel article model and associated stuff for reuse in our projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Joyner",
            "email": "joyner.michael@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/database": "^5.7.0",
        "illuminate/support": "^5.7.0",
        "illuminate/queue": "^5.7.0",
        "spatie/laravel-medialibrary": "^7.0",
        "cviebrock/eloquent-sluggable": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Dymantic\\Articles\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dymantic\\Articles\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dymantic\\Articles\\ArticlesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
